console.log(2 < 2);
console.log(2 > 2);
console.log(2 <= 2);
console.log(2 >= 2);
console.log(2 == 2);

// logical opr and == works differentlly on "null", in logical opr it converts null into numeric but == doesnt//

//BAD PRACTICE//

console.log(null < 0);
console.log(null == 0);
console.log(null >= 0);

// BAD PRACTICE//

console.log("1" < 1);
console.log("01" > 1);

// BAD PRACTICE//

console.log(undefined < 1);
console.log(undefined > 1);
